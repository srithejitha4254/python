    "import socket\n",
    "import time\n",
    "s=socket.socket()\n",
    "s.connect((\"localhost\",2000))\n",
    "while 1:\n",
    "    msg=s.recv(2).decode()\n",
    "    print(\"recieved -->\",int(msg))\n",
    "    s.send(str(msg).encode())\n",
    "    time.sleep(2)\n",
    "s.close()"
   ]
